class OUT is
   create: SAME is
      return self;
   end;

   plus(s: STR) is
      System::Console::Write(s);
   end;

   plus(s: STR): SAME is
      System::Console::Write(s);
      return self;
   end;

   plus(o: $OB) is
      System::Console::Write(o);
   end;

   plus(o: $OB): SAME is
      System::Console::Write(o);
      return self;
   end;
end;
