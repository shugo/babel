class VALUE_HOLDER{T} is
  attr value: T;

  create(value: T): VALUE_HOLDER{T} is
    res ::= new VALUE_HOLDER{T};
    res.value := value;
    return res;
  end;

  get: T is
    return value;
  end;

  set(value: T) is
    self.value := value;
  end;
end;

class MAIN is
  main is
    x: VALUE_HOLDER{STR};
    x := #VALUE_HOLDER{STR}("foo");
    #OUT + x.get + "\n";
    x.set("bar");
    #OUT + x.get + "\n";
  end
end
